<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="captchaVM" type="com.tejaswisapplication.app.modules.captcha.data.viewmodel.CaptchaVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearCaptcha"
        android:layout_height="match_parent"
        android:layout_width="match_parent" android:orientation="vertical">
        <LinearLayout android:id="@+id/linearIPhoneXorne"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_14pxh"
            android:layout_marginStart="@dimen/_14pxh"
            android:layout_marginTop="@dimen/_12pxv"
            android:layout_width="match_parent" android:orientation="horizontal">
            <ImageView android:id="@+id/imageTime"
                android:layout_height="@dimen/_11pxh"
                android:layout_marginBottom="@dimen/_4pxv"
                android:layout_marginStart="@dimen/_12pxh"
                android:layout_marginTop="@dimen/_5pxv"
                android:layout_width="@dimen/_28pxh"
                android:scaleType="centerCrop" circular="@{true}"
                imageRes="@{@drawable/img_time}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_time"/>
            <LinearLayout android:id="@+id/linearRowmobilesignal"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_4pxv"
                android:layout_marginStart="@dimen/_284pxh"
                android:layout_marginTop="@dimen/_5pxv"
                android:layout_weight="0.7" android:layout_width="0dp" android:orientation="horizontal">
                <ImageView android:id="@+id/imageMobileSignal"
                    android:layout_height="@dimen/_10pxh"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="@dimen/_17pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_mobilesignal"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_mobilesignal"/>
                <ImageView android:id="@+id/imageWifi"
                    android:layout_height="@dimen/_10pxh"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_5pxh"
                    android:layout_width="@dimen/_15pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_wifi"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_wifi"/>
                <ProgressBar android:id="@+id/progressBarGroupSixteen"
                    android:indeterminate="false"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_5pxh"
                    android:layout_weight="0.4"
                    android:layout_width="0dp" android:max="100"
                    android:minHeight="@dimen/_11pxv"
                    android:progress="82"
                    android:progressDrawable="@drawable/layer_list_border_black_900_7a_radius_3" style="@style/Widget.AppCompat.ProgressBar.Horizontal"/>
                <ImageView android:id="@+id/imageCombinedShape"
                    android:layout_height="@dimen/_4pxh"
                    android:layout_marginBottom="@dimen/_3pxv"
                    android:layout_marginStart="@dimen/_1pxh"
                    android:layout_marginTop="@dimen/_3pxv"
                    android:layout_width="@dimen/_1pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_combinedshape"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_combinedshape"/>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearColumnrefresh"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_172pxv"
            android:layout_marginEnd="@dimen/_14pxh"
            android:layout_marginStart="@dimen/_14pxh"
            android:layout_marginTop="@dimen/_139pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylewhite_A700">
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearColumnselectallimag"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_7pxh"
                android:layout_marginStart="@dimen/_7pxh"
                android:layout_marginTop="@dimen/_12pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStyleblue_A200">
                <TextView android:ellipsize="end"
                    android:gravity="start"
                    android:id="@+id/txtSelectallimag"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="@dimen/_68pxv"
                    android:layout_marginEnd="@dimen/_22pxh"
                    android:layout_marginStart="@dimen/_22pxh"
                    android:layout_marginTop="@dimen/_27pxv"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{captchaVM.captchaModel.txtSelectallimag}"
                    style="@style/txtRobotomedium18" tools:text="@string/msg_select_all_imag"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumnrectangle"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_7pxh"
                android:layout_marginStart="@dimen/_7pxh"
                android:layout_marginTop="@dimen/_5pxv"
                android:layout_width="match_parent" android:orientation="vertical">
                <LinearLayout android:id="@+id/linearRowrectangle"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageRectangle"
                        android:layout_height="@dimen/_126pxh"
                        android:layout_width="@dimen/_126pxh"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_rectangle}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle"/>
                    <ImageView android:id="@+id/imageRectangleOne"
                        android:layout_height="@dimen/_126pxh"
                        android:layout_marginStart="@dimen/_4pxh"
                        android:layout_width="@dimen/_126pxh"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_rectangle_126x126}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle_126x126"/>
                    <ImageView android:id="@+id/imageRectangleTwo"
                        android:layout_height="@dimen/_126pxh"
                        android:layout_marginStart="@dimen/_4pxh"
                        android:layout_width="@dimen/_126pxh"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_rectangle_1}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle_1"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearRowrectangleThree"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_4pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageRectangleThree"
                        android:layout_height="@dimen/_126pxh"
                        android:layout_width="@dimen/_126pxh"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_rectangle_2}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle_2"/>
                    <FrameLayout
                        android:id="@+id/frameStackrectangleFour"
                        android:layout_height="@dimen/_114pxv"
                        android:layout_marginBottom="@dimen/_12pxv"
                        android:layout_marginStart="@dimen/_4pxh" android:layout_width="@dimen/_114pxh">
                        <ImageView android:id="@+id/imageRectangleFour"
                            android:layout_gravity="bottom|end"
                            android:layout_height="@dimen/_102pxh"
                            android:layout_marginStart="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_10pxv"
                            android:layout_width="@dimen/_102pxh"
                            android:scaleType="fitXY"
                            imageRes="@{@drawable/img_rectangle_102x102}"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle_102x102"/>
                        <ImageButton
                            android:background="@drawable/rectangle_bg_blue_a200_radius_13"
                            android:id="@+id/btnVector"
                            android:layout_gravity="top|start"
                            android:layout_height="@dimen/_26pxh"
                            android:layout_marginBottom="@dimen/_10pxv"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_width="@dimen/_26pxh"
                            android:paddingHorizontal="@dimen/_5pxh"
                            android:paddingVertical="@dimen/_5pxv"
                            android:scaleType="fitCenter"
                            android:src="@drawable/img_vector" tools:ignore="ContentDescription"/>
                    </FrameLayout>
                    <ImageView android:id="@+id/imageRectangleFive"
                        android:layout_height="@dimen/_126pxh"
                        android:layout_marginStart="@dimen/_16pxh"
                        android:layout_width="@dimen/_126pxh"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_rectangle_3}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle_3"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearRowrectangleSix"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_4pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageRectangleSix"
                        android:layout_height="@dimen/_126pxh"
                        android:layout_width="@dimen/_126pxh"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_rectangle_4}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle_4"/>
                    <ImageView android:id="@+id/imageRectangleSeven"
                        android:layout_height="@dimen/_126pxh"
                        android:layout_marginStart="@dimen/_4pxh"
                        android:layout_width="@dimen/_126pxh"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_rectangle_5}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle_5"/>
                    <ImageView android:id="@+id/imageRectangleEight"
                        android:layout_height="@dimen/_126pxh"
                        android:layout_marginStart="@dimen/_4pxh"
                        android:layout_width="@dimen/_126pxh"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_rectangle_6}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle_6"/>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearRowverify"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_11pxv"
                android:layout_marginEnd="@dimen/_13pxh"
                android:layout_marginStart="@dimen/_13pxh"
                android:layout_marginTop="@dimen/_20pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <LinearLayout android:id="@+id/linearRowrefresh"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_4pxv"
                    android:layout_marginTop="@dimen/_3pxv"
                    android:layout_weight="0.49"
                    android:layout_width="0dp" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageRefresh"
                        android:layout_height="@dimen/_26pxh"
                        android:layout_width="@dimen/_25pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_refresh"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_refresh"/>
                    <ImageView android:id="@+id/imageMusic"
                        android:layout_height="@dimen/_19pxh"
                        android:layout_marginBottom="@dimen/_3pxv"
                        android:layout_marginStart="@dimen/_12pxh"
                        android:layout_marginTop="@dimen/_4pxv"
                        android:layout_width="@dimen/_18pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_music"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_music"/>
                    <ImageView android:id="@+id/imageInfo"
                        android:layout_height="@dimen/_20pxh"
                        android:layout_marginBottom="@dimen/_2pxv"
                        android:layout_marginStart="@dimen/_15pxh"
                        android:layout_marginTop="@dimen/_4pxv"
                        android:layout_width="@dimen/_20pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_info"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_info"/>
                </LinearLayout>
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnVerify"
                    android:layout_height="@dimen/_34pxv"
                    android:layout_marginStart="@dimen/_188pxh"
                    android:layout_width="@dimen/_94pxh"
                    android:paddingBottom="@dimen/_10pxv"
                    android:paddingEnd="@dimen/_24pxh"
                    android:paddingStart="@dimen/_23pxh"
                    android:paddingTop="@dimen/_10pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_verify"
                    android:textAllCaps="false"
                    style="@style/btnSolidRounded" tools:text="@string/lbl_verify"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
