<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="cameraAccessVM" type="com.tejaswisapplication.app.modules.cameraaccess.data.viewmodel.CameraAccessVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearCameraAccess"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_50">
        <androidx.appcompat.widget.Toolbar
            android:gravity="center_horizontal"
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_14pxh"
            android:layout_marginStart="@dimen/_21pxh"
            android:layout_marginTop="@dimen/_12pxv"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:id="@+id/linearIPhoneXorne"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageTime"
                    android:layout_height="@dimen/_11pxh"
                    android:layout_marginBottom="@dimen/_4pxv"
                    android:layout_marginStart="@dimen/_12pxh"
                    android:layout_marginTop="@dimen/_5pxv"
                    android:layout_width="@dimen/_28pxh"
                    android:scaleType="centerCrop" circular="@{true}"
                    imageRes="@{@drawable/img_time}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_time"/>
                <LinearLayout android:id="@+id/linearRowmobilesignal"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_4pxv"
                    android:layout_marginStart="@dimen/_284pxh"
                    android:layout_marginTop="@dimen/_5pxv"
                    android:layout_weight="0.7"
                    android:layout_width="0dp" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageMobileSignal"
                        android:layout_height="@dimen/_10pxh"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_width="@dimen/_17pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_mobilesignal"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_mobilesignal"/>
                    <ImageView android:id="@+id/imageWifi"
                        android:layout_height="@dimen/_10pxh"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_5pxh"
                        android:layout_width="@dimen/_15pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_wifi"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_wifi"/>
                    <ProgressBar android:id="@+id/progressBarGroupTwo"
                        android:indeterminate="false"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_5pxh"
                        android:layout_weight="0.4"
                        android:layout_width="0dp" android:max="100"
                        android:minHeight="@dimen/_11pxv"
                        android:progress="82"
                        android:progressDrawable="@drawable/layer_list_border_black_900_7a_radius_3" style="@style/Widget.AppCompat.ProgressBar.Horizontal"/>
                    <ImageView android:id="@+id/imageCombinedShape"
                        android:layout_height="@dimen/_4pxh"
                        android:layout_marginBottom="@dimen/_3pxv"
                        android:layout_marginStart="@dimen/_1pxh"
                        android:layout_marginTop="@dimen/_3pxv"
                        android:layout_width="@dimen/_1pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_combinedshape"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_combinedshape"/>
                </LinearLayout>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearColumndhiwiseappwan"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_389pxv"
            android:layout_marginEnd="@dimen/_21pxh"
            android:layout_marginStart="@dimen/_21pxh"
            android:layout_marginTop="@dimen/_356pxv"
            android:layout_width="wrap_content"
            android:orientation="vertical" style="@style/groupStylewhite_A700cornerRadius">
            <TextView android:ellipsize="end"
                android:id="@+id/txtDhiWiseappwan"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_21pxv"
                android:layout_width="match_parent"
                android:lineSpacingExtra="@dimen/_1pxh"
                android:singleLine="true"
                android:text="@{cameraAccessVM.cameraAccessModel.txtDhiWiseappwan}"
                style="@style/txtGilroymedium16" tools:text="@string/msg_dhiwise_app_wan"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowcamera"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_18pxh"
                android:layout_marginStart="@dimen/_18pxh"
                android:layout_marginTop="@dimen/_15pxv"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <ImageView android:id="@+id/imageCamera"
                    android:layout_height="@dimen/_17pxh"
                    android:layout_width="@dimen/_20pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_camera"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_camera"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtAccessyourcam"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_11pxh"
                    android:layout_marginTop="@dimen/_3pxv"
                    android:layout_weight="0.87"
                    android:layout_width="0dp"
                    android:lineSpacingExtra="@dimen/_1pxh"
                    android:singleLine="true"
                    android:text="@{cameraAccessVM.cameraAccessModel.txtAccessyourcam}"
                    style="@style/txtGilroymedium14" tools:text="@string/msg_access_your_cam"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearRowdeny"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_16pxv"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_27pxv"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnDeny"
                    android:layout_height="@dimen/_34pxv"
                    android:layout_width="@dimen/_134pxh"
                    android:lineSpacingExtra="@dimen/_1pxh"
                    android:paddingBottom="@dimen/_10pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_10pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_deny"
                    android:textAllCaps="false"
                    style="@style/btnRoundedOutline" tools:text="@string/lbl_deny"/>
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnAllow"
                    android:layout_height="@dimen/_34pxv"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_width="@dimen/_134pxh"
                    android:lineSpacingExtra="@dimen/_1pxh"
                    android:paddingBottom="@dimen/_10pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_10pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_allow"
                    android:textAllCaps="false"
                    style="@style/btnSolidRounded_1" tools:text="@string/lbl_allow"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
